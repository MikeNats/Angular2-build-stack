{
  "name": "angular2-build-stack",
  "version": "1.0.0",
  "description": "Angular2, Grunt build stack",
  "scripts": {
    "start": "grunt && lite-server",
    "pree2e": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/webdriver-manager start",
    "e2e": "concurrently  \"lite-server -s\" \"protractor protractor.conf.js\" --kill-others --success first",
    "deploy": "grunt deploy",
    "build": "grunt",
    "testDev": "karma start",
    "test": "grunt test"
  },
  "keywords": ["angular2", "typescipt", "grunt", "browserify", "tsify", "rxjs", "protractor", "karma", "jasmine"],
  "author": "Michail Tsougkranis",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular-in-memory-web-api": "~0.1.0",
    "core-js": "^2.4.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "foundation-sites": "^6.2.3",
    "lodash": "^4.16.1",
    "rxjs": "5.0.0-beta.12"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "canonical-path": "0.0.2",
    "concurrently": "^2.2.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-html-minify": "^0.3.1",
    "grunt-karma": "^2.0.0",
    "grunt-ts": "^6.0.0-beta.3",
    "grunt-tslint": "^3.2.1",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lite-server": "^2.2.2",
    "load-grunt-tasks": "^3.5.2",
    "lodash": "^4.16.1",
    "phantomjs": "^2.1.7",
    "protractor": "^3.3.0",
    "reflect-metadata": "^0.1.3",
    "tsify": "^1.0.7",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webdriver-manager": "^10.2.3",
    "zone.js": "^0.6.23"
  }
}
